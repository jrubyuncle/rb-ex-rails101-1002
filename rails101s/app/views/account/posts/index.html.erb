<div class="col-md-12">
  <h2 class="text-center"></h2>

  <table class="table">
    <thead>
      <tr>
        <th>#</th>
        <th>Content</th>
        <th>Group ID</th>
        <th>User ID</th>
        <th>Last Update</th>
      </tr>
    </thead>

    <tbody>
      <% @posts.each do |post| %>
        <tr>
          <td><%= post.id %></td>
          <td><%= post.content %></td>
          <td><%= post.group_id %></td>
          <td><%= post.user_id %></td>
          <td><%= post.updated_at %></td>
        </tr>
      <% end %>
    </tbody>


  </table>





</div>